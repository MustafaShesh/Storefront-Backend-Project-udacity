{
  "name": "store-front-backend-projects",
  "version": "1.0.0",
  "description": "Store-front API",
  "main": "index.js",
  "scripts": {
    "watch": "tsc-watch --esModuleInterop \"src/server.ts\" --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "start": "nodemon build/server.js",
    "dev": "npm run watch && npm run start",
    "test": "set ENV=test db-migrate --env db:up test && db-migrate db:drop test",
    "test2": "npm run build && npm run jasmine",
    "build": "npx tsc",
    "jasmine": "jasmine",
    "lint": "eslint --fix \"./**/*.ts\"  ",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" \"**/*.js\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MustafaShesh/StorefrontBackendUdacity.git"
  },
  "keywords": [],
  "author": "Mustafa Gamal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MustafaShesh/StorefrontBackendUdacity/issues"
  },
  "homepage": "https://github.com/MustafaShesh/StorefrontBackendUdacity#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/pg": "^8.6.5",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0"
  }
}
